<!--pages/circle/circle.wxml-->
<view class="page" data-weui-theme="{{theme}}">
    <view class="page__hd">
        <view class="page__title">圈子</view>
    </view>
    <view class="page__bd">
        <view class="weui-cells weui-cells_after-title">
            <navigator url="./shareApp" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
                <view class="weui-cell__hd">
                    <image src="../../images/zf.png"></image>
                </view>
                <view class="weui-cell__bd">实时分享</view>
                <view class="weui-cell__ft weui-cell__ft_in-access"></view>
            </navigator>

            <navigator url="./ranking" class="weui-cell weui-cell_example weui-cell_access" hover-class="weui-cell_active">
                <view class="weui-cell__hd">
                    <image src="../../images/prime.PNG"></image>
                </view>
                <view class="weui-cell__bd">我的排名</view>
                <view class="weui-cell__ft weui-cell__ft_in-access"></view>
            </navigator>

            <navigator url="{{defaultUrl}}" class="weui-cell weui-cell_example weui-cell_access" hover-class="weui-cell_active">
                <view class="weui-cell__hd">
                    <image src="../../images/communit.PNG"></image>
                </view>
                <view class="weui-cell__bd">社区交流</view>
                <view class="weui-cell__ft weui-cell__ft_in-access"></view>
            </navigator>

            <navigator url="./achive" class="weui-cell weui-cell_example weui-cell_access" hover-class="weui-cell_active">
                <view class="weui-cell__hd">
                    <image src="../../images/achive.PNG"></image>
                </view>
                <view class="weui-cell__bd">我的成就</view>
                <view class="weui-cell__ft weui-cell__ft_in-access"></view>
            </navigator>

        </view>
    </view>
</view>
