
<view class="page" data-weui-theme="{{theme}}">
    <view class="userinfo">
        <open-data class="userinfo-avatar" type="userAvatarUrl"></open-data>
        <open-data class=userinfo-nickname" type="userNickName"></open-data>
    </view>
    <view class="page__bd">
        <view class="weui-cells weui-cells_after-title">
            <navigator url="{{defaultUrl}}" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
                <view class="weui-cell__hd">
                    <image src="../../images/sc.png"></image>
                </view>
                <view class="weui-cell__bd">我的收藏</view>
                <view class="weui-cell__ft weui-cell__ft_in-access"></view>
            </navigator>

            <navigator url="{{defaultUrl}}" class="weui-cell weui-cell_example weui-cell_access" hover-class="weui-cell_active">
                <view class="weui-cell__hd">
                    <image src="../../images/video.PNG"></image>
                </view>
                <view class="weui-cell__bd">视频讲解</view>
                <view class="weui-cell__ft weui-cell__ft_in-access"></view>
            </navigator>

            <navigator url="./sys_setting" class="weui-cell weui-cell_example weui-cell_access" hover-class="weui-cell_active">
                <view class="weui-cell__hd">
                    <image src="../../images/settings.png"></image>
                </view>
                <view class="weui-cell__bd">系统设置</view>
                <view class="weui-cell__ft weui-cell__ft_in-access"></view>
            </navigator>

            <navigator url="./about" class="weui-cell weui-cell_example weui-cell_access" hover-class="weui-cell_active">
                <view class="weui-cell__hd">
                    <image src="../../images/about.png"></image>
                </view>
                <view class="weui-cell__bd">关于我们</view>
                <view class="weui-cell__ft weui-cell__ft_in-access"></view>
            </navigator>

        </view>
    </view>
</view>
